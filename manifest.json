    "focus-sibling-number-1": {},
    "focus-sibling-number-2": {},
    "focus-sibling-number-3": {},
    "focus-sibling-number-4": {},
    "focus-sibling-number-5": {},
    "focus-sibling-number-6": {},
    "focus-sibling-number-7": {},
    "focus-sibling-number-8": {},
    "focus-sibling-number-9": {},
    "focus-sibling-number-10": {},
    "focus-sibling-number-11": {},
    "focus-sibling-number-12": {},
    "focus-sibling-number-13": {},
    "focus-sibling-number-14": {},
    "focus-sibling-number-15": {},
    "focus-sibling-number-16": {},
    "focus-sibling-number-17": {},
    "focus-sibling-number-18": {},
