{
  "manifest_version": 2,
  "name": "TST Number Navigation",
  "version": "2.0",

  "description": "Adds numberish shortcuts to move through Tree Style Tabs.",

  "browser_specific_settings": {
    "gecko": {
      "id": "tst-num-nav@example.com"
    }
